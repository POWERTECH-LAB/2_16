@Name('FIRST_TICK')
@Priority(2)
select
    engineStrategy.name as strategyName,
    tick
from
    ValidTick(security.id = ?).std:firstevent() as tick;

@Name('AFTER_TRADE')
select
    orderStatus
from
    pattern[[?] (orderStatus=OrderStatus(status = Status.EXECUTED or status = Status.CANCELED, parentOrder.security.id in (?)))]
