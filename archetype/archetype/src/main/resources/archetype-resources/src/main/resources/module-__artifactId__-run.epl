//Example
@Name('OPEN_POSITION')
@Subscriber(className='${package}.${nameOfService}Service.openPosition')
select
    engineStrategy.name as strategyName,
    indexTick.security.id as underlyingid,
    indexTick.currentValue as underlyingSpot
from
    pattern [every (indexTick=Tick(security.isin=securityId)
    -> indicator=Indicator)]
where
    indicator.value > 0
and
    prior(1, indicator.value) <= 0;
