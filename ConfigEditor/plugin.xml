<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="configeditor.EditorPropertyPage"
            id="ConfigEditor.page"
            name="Config Editor">
         <enabledWhen>
             <or>
                <instanceof
                      value="org.eclipse.core.resources.IProject">
                </instanceof>
                <instanceof
                      value="org.eclipse.jdt.core.IJavaProject">
                </instanceof>
            </or>
         </enabledWhen>
      </page>
   </extension>

</plugin>
